# ParkAlgo Environment Variables Example
# IMPORTANT: DO NOT ADD REAL SECRETS TO THIS FILE
# Copy this file to .env.local and fill in actual values there

# Stripe API keys (required for payment processing)
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_webhook_secret

# Stripe Product IDs
NEXT_PUBLIC_STRIPE_NAVIGATOR_PRICE_ID=your_navigator_price_id
NEXT_PUBLIC_STRIPE_PRO_PARKER_PRICE_ID=your_pro_parker_price_id
NEXT_PUBLIC_STRIPE_FLEET_MANAGER_PRICE_ID=your_fleet_manager_price_id

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_key

# Other API Keys
NEXT_PUBLIC_MAPBOX_TOKEN=your_mapbox_token
NEXT_PUBLIC_GOOGLE_MAPS_KEY=your_google_maps_key

# Feature Flags
NEXT_PUBLIC_ENABLE_AI_PREDICTIONS=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# REMINDER: NEVER COMMIT .env.local OR ANY FILE CONTAINING REAL SECRETS
# THIS IS JUST A TEMPLATE WITH PLACEHOLDER VALUES
